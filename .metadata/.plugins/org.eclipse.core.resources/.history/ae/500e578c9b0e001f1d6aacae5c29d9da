        super.onCreate(savedInstanceState);
        setContentView(R.layout.activity_success);
    }

    @Override
    public boolean onCreateOptionsMenu(Menu menu) {
        getMenuInflater().inflate(R.menu.activity_success, menu);
        return true;
    }
}
